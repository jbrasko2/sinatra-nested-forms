<h1>Pirate</h1>
<h3>Pirate Name</h3>
<p><%= @pirate.name %></p>
<h3>Pirate Weight</h3>
<p><%= @pirate.weight %></p>
<h3>Pirate Height</h3>
<p><%= @pirate.height %></p>
<br>


<h1>Ships</h1>
<% @ships.each do |ship| %>
    <p>Name: <%= ship.name %></p>
    <p>Type: <%= ship.type %></p>
    <p>Booty: <%= ship.booty %></p>
<% end %>   
